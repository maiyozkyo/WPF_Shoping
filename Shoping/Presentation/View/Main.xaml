<Window x:Class="Shoping.Presentation.View.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shoping.Presentation.View"
        mc:Ignorable="d"
        Title="Main" Height="450" Width="800">
    <Grid>
        <Button 
            Content="Button" 
            HorizontalAlignment="Left" 
            Margin="281,199,0,0" 
            VerticalAlignment="Top" Click="Button_Click"
            
        />
        <TextBox HorizontalAlignment="Left" Margin="281,157,0,0" TextWrapping="Wrap" Text="{Binding PageData.Total}" VerticalAlignment="Top" Width="120"/>
        <Button Content="Excel" HorizontalAlignment="Left" Margin="369,209,0,0" VerticalAlignment="Top" Click="Excel_Click"/>

    </Grid>
</Window>
